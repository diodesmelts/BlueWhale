{
  "version": 2,
  "framework": null,
  "buildCommand": "node vercel-build.js",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.js" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}