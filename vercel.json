{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/[...slug].js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/[...slug].js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ],
  "github": {
    "enabled": true,
    "silent": true
  }
}