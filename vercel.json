{
  "version": 2,
  "buildCommand": "node vercel-build.js",
  "devCommand": "npm run dev",
  "outputDirectory": "dist",
  "installCommand": "npm install --force",
  "framework": null,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}